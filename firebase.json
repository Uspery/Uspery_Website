{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/ServiceDeskManager",
        "destination": "/projects/ServiceDeskManager/index.html"
      },
      {
        "source": "/sdm",
        "destination": "/projects/ServiceDeskManager/index.html"
      },
      {
        "source": "/PalacePetz",
        "destination": "/projects/PalacePetz/index.html"
      },
      {
        "source": "/privacidade/*",
        "destination": "/privacy/index.html"
      },
      {
        "source": "/privacy/*",
        "destination": "/privacy/index.html"
      },
      {
        "source": "/tos",
        "destination": "/privacy/tos/index.html"
      },
      {
        "source": "/tos/",
        "destination": "/privacy/tos/index.html"
      },
      {
        "source": "/about/*",
        "destination": "/index.html"
      },
      {
        "source": "/sobre/*",
        "destination": "/index.html"
      },
      {
        "source": "/short/**",
        "dynamicLinks": true
      },
      {
        "source": "/url/**",
        "dynamicLinks": true
      }
    ],
    "cleanUrls": true,
    "headers": [{
      "source" : "**",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=86400"
      }]
    }]
  }
}
